{
    "Literal": {
        "value": {
            "satisfies": "ANY", 
            "rules": [
                {"F": {"typeof": "number"}},
                {"F": {"equals": null}}
            ]
        }
    },
    "BinaryExpression": {
        "operator": {
            "satisfies": "ANY", 
            "rules": [
                {"F": {"equals": "+"}}, 
                {"F": {"equals": "-"}}
            ]
        } 
    },
    "ArrayExpression": {
        "elements": {
            "satisfies": "ANY", 
            "rules": [{"P": {"length": {"F": {"equals": 2}}}}]
        }
    },
    "ArrowFunctionExpression": {
        "params": {
            "satisfies": "ANY", 
            "rules": [{"P": {"length": {"F": {"equals": 1}}}}]
        }
    },
    "CallExpression": {
        "arguments": {
            "satisfies": "ANY",
            "rules": [{"P": {"length": {"F": {"equals": 1}}}}]
        }
    },
    "ConditionalExpression": {
        "test": {
            "satisfies": "ALL",
            "rules": [
                {"P": {"type": {"F": {"equals": "BinaryExpression"}}}},
                {"P": {"operator": {"F": {"equals": "<="}}}},
                {"P": {"right": {"P": {"raw": {"F": {"equals": "0"}}}}}}
            ]
        }
    },
    "Identifier": {
        "name": {
            "satisfies": "ANY",
            "rules": [
                {"P": {"length": {"F": {"typeof": "number"}}}}
            ]
        }
    },
    "VariableDeclaration":{
        "kind":{
            "satisfies": "ALL",
            "rules": [
                {"F": {"equals": "const"}}
            ]
        },
        "declarations": {
            "satisfies": "ALL",
            "rules": [
                {"P": {"length": {"F": {"equals": 1}}}},
                {"P": {"0": {"P": {"type": {"F": {"equals": "VariableDeclarator"}}}}}}
            ]
        }
        
    },
    "BlockStatement":{
        "body": {
            "satisfies": "NONE",
            "rules": [
                {"P": {"length": {"F": {"equals": 0}}}}
            ]
        },
        "body%": {
            "satisfies": "ALL",
            "rules": [
                {"P": {"%last": {"P": {"type": {"F": {"equals": "ReturnStatement"}}}}}}
            ]
        }
    },
    "ReturnStatement":{
        "argument":{
            "satisfies": "NONE",
            "rules": [
                {"F": {"typeof": "null"}}
            ]
        }
    },
    "ExpressionStatement":{
        "type":{
            "satisfies": "ANY",
            "rules": [
                {"F": {"equals": "ExpressionStatement"}}
            ]
        }
    },
    "Program":{
        "sourceType":{
            "satisfies": "ALL",
            "rules": [
                {"F": {"equals": "script"}}
            ]
        },
        "body":{
            "satisfies": "NONE",
            "rules": [
                {"P": {"length": {"F": {"equals": 0}}}}
            ]
        }
    }
}